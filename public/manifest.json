{
  "manifest_version": 3,
  "name": "The Joker",
  "description": "This app is a joker app made by sweetkoffi. Allow your to send request to GPT-Turbo",
  "version": "1.0.0",
  "action": {
      "default_popup": "index.html",
      "default_title": "Open the popup"
  },
  "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "permissions": [
      "scripting",
      "activeTab",
      "storage"
  ],
  "host_permissions": [
      "https://*/"
  ]
}